<aura:interface description="PaymentMethodsInterface" access="global">
  <aura:attribute name="contactId" type="String" required="true" access="global"/>
  <aura:attribute name="contactName" type="String" access="global"/>
  <aura:attribute name="paymentGateway" type="Map" access="global"/>
  <aura:attribute name="paymentMethods" type="Object[]" default="[]" access="global"/>
  <aura:attribute name="componentToCreate" type="String" required="true" access="global"/>
  <aura:registerEvent name="PaymentMethodEvent" type="OrderApi:PaymentMethodEvent"/>
</aura:interface>