<aura:component description="EventPageName">
    <aura:attribute name="pageName" type="String" required="true"/>
    <aura:attribute name="pageDisplayName" type="String" default=""/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <div class="slds-truncate">{!v.pageDisplayName}</div>
</aura:component>